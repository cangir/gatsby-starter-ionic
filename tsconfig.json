{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowJs": true,
    "noImplicitReturns": true,
  },
  "exclude": ["node_modules", "public", ".cache"],
  "include": [
    "./src/**/*"
  ]
}